<?xml version="1.0"?>
<!-- This is the ES-DE find rules configuration file for Unix -->
<ruleList>
<emulator name="YUZU">
	<!-- Nintendo Switch emulator Yuzu -->
	<rule type="staticpath">
		<entry>%ESPATH%\Emulators\yuzu\yuzu.exe</entry>
		<entry>%ESPATH%\..\Emulators\yuzu\yuzu.exe</entry>
	</rule>
</emulator>
